{
  "name": "change-of-pace",
  "type": "module",
  "version": "7.1.0",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build && npm run ogp-sort",
    "preview": "astro preview",
    "check": "astro check",
    "astro": "astro",
    "ogp-sort": "node generated/ogp/ogp-sort.mjs",
    "lint-check": "npm run lint:eslint && npm run lint:stylelint && npm run check:prettier",
    "lint:eslint": "eslint \"./src/**/*.{js,jsx,ts,tsx,astro}\"",
    "lint:stylelint": "stylelint \"./src/**/*.scss\"",
    "lint:textlint:full": "textlint --cache \"src/content/**/*.{md,mdx}\" -f pretty-error",
    "check:prettier": "prettier --check \"./src/**/*.{js,jsx,ts,tsx,astro,html,json,yml}\"",
    "fix": "npm run fix:eslint && npm run fix:stylelint && npm run fix:prettier",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:stylelint": "npm run lint:stylelint --fix",
    "fix:textlint:full": "textlint --cache \"src/content/**/*.{md,mdx}\" --fix",
    "fix:prettier": "npm run check:prettier -- --write",
    "prepare": "lefthook install || echo 'Can not set git hooks'"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^4.3.5",
    "@astrojs/partytown": "^2.1.4",
    "@astrojs/rss": "^4.0.12",
    "@astrojs/sitemap": "^3.5.1",
    "@iconify-json/logos": "^1.2.4",
    "@iconify-json/ri": "^1.2.5",
    "@iconify-json/system-uicons": "^1.2.2",
    "@iconify-json/teenyicons": "^1.2.2",
    "@unocss/reset": "^66.5.1",
    "astro": "^5.13.7",
    "cheerio": "^1.1.2",
    "markdown-it": "^14.1.0",
    "rehype-autolink-headings": "^7.1.0",
    "remark-breaks": "^4.0.0",
    "remark-flexible-code-titles": "^1.3.1",
    "sanitize-html": "^2.17.0",
    "sass": "^1.87.0",
    "wicg-inert": "^3.1.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@types/markdown-it": "^14.1.2",
    "@types/sanitize-html": "^2.16.0",
    "@unocss/astro": "^66.5.1",
    "@unocss/autocomplete": "^66.5.1",
    "@unocss/eslint-config": "^66.5.1",
    "eslint": "^9.25.1",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "lefthook": "^1.11.11",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "stylelint": "^16.19.1",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-recess-order": "^6.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-order": "^7.0.0",
    "textlint": "^14.6.0",
    "textlint-filter-rule-comments": "^1.2.2",
    "textlint-rule-preset-ja-technical-writing": "^12.0.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0",
    "unocss": "^66.5.1"
  },
  "volta": {
    "node": "22.15.0"
  }
}
